{
  "name": "@sherwinski/pokeapi-ts",
  "version": "1.1.2",
  "description": "A TypeScript SDK that wraps the RESTful Pokemon API for all things from the Pokemon world and games",
  "main": "dist/src/Pokedex.js",
  "types": "dist/src/Pokedex.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "prebuild": "pnpm run clean",
    "build": "pnpm run prebuild && tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "Sherwin Heydarbeygi <https://github.com/sherwinski>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "types",
    "README",
    "LICENSE",
    "CHANGELOG"
  ],
  "dependencies": {
    "pokenode-ts": "^1.20.0"
  }
}
